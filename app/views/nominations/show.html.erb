<div class="page-header">
  <h3>Nomination #<%= @nomination.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Book</dt>
      <dd><%= @nomination.book.title %></dd>

      <dt>Meeting</dt>
      <dd><%= @nomination.month.started_on.strftime("%B %Y") %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/nominations" class="btn btn-primary">
        Back
      </a>
    
      <a href="/delete_nomination/<%= @nomination.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
