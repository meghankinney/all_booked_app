<div class="page-header">
  <h1>
    Votes

  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Nomination Title</th>
        <th>Voted By</th>
        <th>Actions</th>
      </tr>

      <% @votes.each do |vote| %>
      <tr>
        <td><%= vote.nomination.book.title %></td>
        <td><%= vote.user.username %></td>
        <td>
          <a href="/votes/<%= vote.id %>" class="btn btn-primary">Show</a>

          <a href="/delete_vote/<%= vote.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
