<div class="page-header">
  <h3>Vote #<%= @vote.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Nomination</dt>
      <dd><%= @vote.nomination.book.title %></dd>

      <dt>User</dt>
      <dd><%= @vote.user.username %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/votes" class="btn btn-primary">
        Back
      </a>
      
      <a href="/delete_vote/<%= @vote.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
